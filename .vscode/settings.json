{
    "forbeslindesay-taskrunner.source": "Workspace",
    "cSpell.words": [
        "ASPNETCORE",
        "beforeunload",
        "easymde",
        "Hasher",
        "htmx",
        "iframe",
        "outfile",
        "signalr",
        "wikibase",
        "Wikirace"
    ],
    "[aspnetcorerazor]": {
        "editor.defaultFormatter": null,
        "editor.formatOnSave": false,
        "editor.wordWrap": "wordWrapColumn"
    },
    "razor.format.enable": false,
    "sqltools.connections": [
        {
            "previewLimit": 50,
            "driver": "SQLite",
            "database": "wikirace.db",
            "name": "Wikirace"
        }
    ],
    "workbench.iconTheme": "vscode-icon-theme",

    "files.associations": {
        "libman.json": "jsonc",
        "appsettings.json": "jsonc",
        "appsettings.*.json": "jsonc"
    },
    "terminal.integrated.env.linux": {
        "ASPNETCORE_ENVIRONMENT": "Development"
    }
}
