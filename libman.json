{
    "version": "1.0",
    "defaultProvider": "unpkg",
    "libraries": [
        {
            "library": "bootstrap@5.3.2",
            "destination": "src/wwwroot/lib/bootstrap",
            "files": ["dist/css/*", "dist/js/*"]
        },
        {
            "library": "jquery@3.7.1",
            "destination": "src/wwwroot/lib/jquery",
            "files": ["dist/*"]
        },
        {
            "library": "jquery-validation@1.20.0",
            "destination": "src/wwwroot/lib/jquery-validation",
            "files": ["dist/*"]
        },
        {
            "library": "jquery-validation-unobtrusive@4.0.0",
            "destination": "src/wwwroot/lib/jquery-validation-unobtrusive",
            "files": ["dist/*"]
        },
        {
            "library": "htmx.org@1.9.10",
            "destination": "src/wwwroot/lib/htmx",
            "files": ["dist/ext/sse.js", "dist/htmx.js"]
        },
        {
            "library": "canvas-confetti@1.9.2",
            "destination": "src/wwwroot/lib/canvas-confetti",
            "files": ["dist/confetti.browser.min.js"]
        }
    ]
}
